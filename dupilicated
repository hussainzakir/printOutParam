			<tr><td>Total Amount Invoiced from Hauler:</td><td><c:out value="${ totalInvoicedFromHauler}"/></td></tr>
			<tr><td>Total Amount to be Paid to Hauler:</td><td><c:out value="${totalPaidToHauler }"/></td></tr>
		</table>
		</div>
	</div>
</div>
<script>
